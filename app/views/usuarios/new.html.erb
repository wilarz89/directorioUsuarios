<h1>Nuevo Usuario</h1>

<%= form_for :usuario , url: usuarios_path do |f| %>
  <p>
    <%= f.label :Nombres %><br>
    <%= f.text_field :names %>
  </p>

  <p>
    <%= f.label :Apellidos %><br>
    <%= f.text_area :apellidos %>
  </p>

  <p>
    <%= f.label :Email %><br>
    <%= f.text_area :email %>
  </p>

  <p>
    <%= f.label :Telefono %><br>
    <%= f.text_area :telefono %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
